<manifest android="http://schemas.android.com/apk/res/android" package="com.xinai.djx">
    <uses-permission name="android.permission.WAKE_LOCK"/>
    <uses-permission name="android.permission.INTERNET"/>
    <uses-permission name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <uses-permission name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission name="android.permission.RECORD_AUDIO"/>
    <uses-permission name="android.permission.MODIFY_AUDIO_SETTINGS"/>
    <uses-permission name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission name="android.permission.READ_PHONE_STATE"/>
    <uses-permission name="android.permission.VIBRATE"/>
    <uses-permission name="android.permission.REQUEST_INSTALL_PACKAGES"/>
    <application name="demo.GameApplicateion" allowBackup="true" icon="@mipmap/ic_launcher" label="@string/app_name"
                 theme="@style/Theme.AppCompat.NoActionBar" usesCleartextTraffic="true">
        <activity name="demo.GameSplashActivity" screenOrientation="portrait"
                  windowSoftInputMode="stateHidden|adjustPan"
                  configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation" label="@string/app_name">
            <intent-filter>
                <action name="android.intent.action.MAIN"/>
                <category name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <meta-data name="splash_activity" value="true"/>
        </activity>
        <activity name="demo.MainActivity" screenOrientation="portrait" windowSoftInputMode="stateHidden|adjustPan"
                  configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation" label="@string/app_name">
            <meta-data name="game_activity" value="true"/>
        </activity>
        <uses-library name="org.apache.http.legacy" required="false"/>
        <meta-data name="xinhun_appid" value="11111"/>
        <meta-data name="xinhun_appkey" value="2222"/>
        <meta-data name="xinhun_data_id" value="3333333"/>
    </application>
</manifest>
